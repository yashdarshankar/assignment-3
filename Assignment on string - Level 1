def check_palindrome(word):
    word1 = ""
    for i in range(len(word)-1,-1,-1):
        word1 += word[i]
    if(word == word1):
        return True
    else:
        return False
    #Remove pass and write your logic here

status=check_palindrome("malayalam")

if(status):
    print("word is palindrome")
else:
    print("word is not palindrome")
